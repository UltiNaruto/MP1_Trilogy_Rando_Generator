<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="wii" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    <section name="gamecube" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    <section name="junkIdSubstitutions" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    <section name="junkStartOffset" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    <section name="junkRedumpPatch" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
    <section name="preserveFstFileAlignment" type="System.Configuration.AppSettingsSection, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
  </configSections>

  <!-- Linux Users: \ in paths are replaced for / by the app -->
  <!-- Starting a path with ~ will insert the home path (all OSs) -->

  <!-- ################## Global ################## -->
  <appSettings>
    <!-- Search sub folders when finding images to process -->
    <add key="SearchSubfolders" value="false"/>
    <!-- Wait for newline entry before exiting a commandline app -->
    <add key="WaitForKeyAfterProcessing" value="true"/>

    <!-- All paths are relative to the working folder - global substitutions %exe=exe path, %pth=Path value -->
    <!-- A base path that can be used in other paths with %pth, paths can be absolute or relative to the working folder - not supported by apps above -->
    <add key="Path" value="%exe\Processed"/>
    <!-- Temp working folder, absolute or relative to current working directory - best to be the same drive as the output -->
    <add key="TempPath" value="%pth\Temp"/>

    <!-- Summary log path, a log of all conversions with image details - check this file before deleting source files etc -->
    <add key="SummaryLog" value="%pth\NKitSummary.txt"/>
    <add key="EnableSummaryLog" value="true"/>

    <!-- Verify the output file by converting it back to iso in memory to ensure the CRC is valid -->
    <add key="FullVerify" value="true"/>
    <!-- Calculates additional MD5 and SHA1 hashes. This is required to output them in to the summary log or use them in the rename file masks -->
    <add key="CalculateHashes" value="true" />

    <!-- Save NKit as iso or gcz -->
    <add key="NkitFormat" value="gcz" />
    <!-- Force a full reencode when converting nkit to nkit. If false when converting nkit.iso to nkit.gcz, only the gcz coversion is performed -->
    <add key="NkitReencode" value="false" />
    <!-- Rename output images to the masks in the gamecube and wii config sections, else use the source path and filename -->
    <add key="MaskRename" value="true"/>
    <!-- Convert images, but delete them when complete - View SummaryLog for results -->
    <add key="TestMode" value="false"/>
    <!-- Recovery Only - delete failed matches, false will rename them to the FailMatchRenameToMask - they may be corrupt -->
    <add key="RecoveryMatchFailDelete" value="true"/>
    <!-- Delete the source file if FullVerify returns VerifySuccess (FullVerify setting must be true) TestMode=true cancels this setting -->
    <add key="DeleteSource" value="false" />

    <!-- Logging level when processing 0=Silent, 1=Info, 2=Detailed -->
    <add key="OutputLevel" value="2"/>

    <!-- Path and mask to GameTDB file - latest file is used-->
    <add key="DatPathNameGameTdbMask" value="%exe\Dats\GameTdb\*.txt"/>
  </appSettings>


  <!-- ################## GAMECUBE ################## -->
  <gamecube>
    <!-- Path and mask to redump and secondary custom dat files - latest file is used -->
    <add key="DatPathRedumpMask" value="%exe\Dats\Redump\GameCube\*.dat"/>
    <add key="DatPathCustomMask" value="%exe\Dats\Custom\GameCube\*.dat"/>
    
    <!-- <Blank> = input name with dest ext. 
         Mask: %src=path of the source image, %nmo=original, %nmd=discTitle, %nmg=gametdb, %nmm=matched (falls back to %nmg then %nmo)
         The tags below are wrapped with [] or () matching the description. If they exist in the filename before substitution they are excluded
               %dno=(discNo not no. 1), %rev=(Revision not Rev 0), %ver=(Version not 1.00), %ext=extension, %crc=[crc32], %md5=[md5], %sha=[sha1], %id4=[id4], %id6=[id6], %id8=[id8]
         Can be prefixed with a path that contains masks also -->
    <add key="RedumpMatchRenameToMask" value="%pth\GameCube\%nmm.%ext"/>
    <add key="CustomMatchRenameToMask" value="%pth\GameCube_Custom\%nmm.%ext"/>
    <add key="MatchFailRenameToMask" value="%pth\GameCube_MatchFail\%nmg %id6 %crc[b].%ext"/>

    <!-- Redump and other appldr.bin and fst.bin recovery files - extracted by NKit, if not found only unscrub can be performed -->
    <add key="RecoveryFilesPath" value="%exe\Recovery\Redump\GameCube"/>
    <add key="OtherRecoveryFilesPath" value="%exe\Recovery\Other\GameCube"/>

